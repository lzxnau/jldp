#! /usr/bin/bash

cd $JLDPDIR/pne || exit
export PNEIMG=pne:3.11.7.20.10.0.29.1

docker run -it --rm --name MDB -h MDB \
       --cpus=4 --memory=16g \
       --network dnet --ip 10.10.0.3 \
       -v $JLDPDIR/env:/home/pne/env \
       -v $JLDPDIR:/home/pne/jldp \
       -e TZ=Australia/Sydney \
       $PNEIMG bash /home/pne/env/mdb.ini

cd ..
